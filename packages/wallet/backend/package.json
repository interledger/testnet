    "awilix": "^10.0.2",
    "axios": "^1.7.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "disposable-email-domains": "^1.0.62",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "hash-wasm": "^4.11.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "iron-session": "^6.3.1",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
