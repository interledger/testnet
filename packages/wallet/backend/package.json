    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.15.1",
